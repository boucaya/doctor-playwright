# Copy this file to .env and fill with your real values. DO NOT commit .env to git.

# Email sender (must match SMTP account)
EMAIL_FROM=you@example.com
# Recipient (can be same as EMAIL_FROM)
EMAIL_TO=you@example.com

# SMTP server settings
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
# Use an app password (recommended) if your provider supports 2FA
EMAIL_PASSWORD=your_app_password_here

# Optional overrides
CHECKER_URL=https://www.centromed.cl/reserva-de-horas/
ARTIFACTS_DIR=./artifacts

# Example target doctor and monitoring settings
# TARGET_DOCTOR is REQUIRED when running with --monitor
#TARGET_DOCTOR=RODRIGO ALVAREZ LOBO
#MAX_DAYS=365
#INTERVAL_SECONDS=300
